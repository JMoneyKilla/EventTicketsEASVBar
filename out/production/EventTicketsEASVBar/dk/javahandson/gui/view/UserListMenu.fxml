        <columns>
          <TableColumn fx:id="tableColumnName" prefWidth="203.33328247070312" text="Name" />
          <TableColumn fx:id="tableColumnEmail" prefWidth="211.3333740234375" text="Email" />
        </columns>
      </TableView>
      <Button layoutX="403.0" layoutY="389.0" mnemonicParsing="false" onAction="#clickDeleteUser" styleClass="buttonSmall" stylesheets="@Menu.css" text="Delete User" />
      <Button layoutX="215.0" layoutY="389.0" mnemonicParsing="false" onAction="#clickCreateUser" styleClass="buttonSmall" stylesheets="@Menu.css" text="Create User" />
      <Button layoutX="319.0" layoutY="389.0" mnemonicParsing="false" onAction="#clickEditUser" styleClass="buttonSmall" stylesheets="@Menu.css" text="Edit User" />
      <Label fx:id="lblWarning" layoutX="76.0" layoutY="393.0" prefHeight="17.0" prefWidth="162.0" textFill="WHITE" />
   </children>
